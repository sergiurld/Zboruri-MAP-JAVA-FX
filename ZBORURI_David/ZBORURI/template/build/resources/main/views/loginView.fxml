   <children>
      <Label text="Hello, Please LOGIN " layoutX="92.0" layoutY="10.0"  />
      <TextField fx:id="usernameField" layoutX="65.0" layoutY="30.0" />
      <Button fx:id="loginButton" onAction="#handleLogin" layoutX="114.0" layoutY="76.0" mnemonicParsing="false" text="LOGIN" style="-fx-background-color: #40E0D0;"/>
      <Button fx:id="infoButton" onAction="#handleInfo" layoutX="236.0" layoutY="110.0" mnemonicParsing="false" text="INFO" style="-fx-background-color: #FF0000;"/>
   </children>
</AnchorPane>
